#!/usr/bin/env node

var Simple = require('../lib/simple').Simple;

var argv = process.argv;
var me   = argv[1];
var name = argv[2] || 'simple';
var port = argv[3] || 5050;
var host = argv[4] || '127.0.0.1';

console.log(argv);

var simple = new Simple({
  name : name,
  port : port,
  host : host
});

simple.start();
